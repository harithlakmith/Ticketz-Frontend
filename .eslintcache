ge(imgdata,'PNG', marginX,marginY, canvasWidth, canvasHeight)\r\n        doc.save(\"Ticket.pdf\")\r\n        }) \r\n   }\r\n        \r\n       \r\n\r\nrender(){\r\n\r\n  const {success,ticket,userInfo,sms} = this.state;\r\n  const pdfname = 'Ticketz-#'+ ticket.tid+'_'+userInfo.Id+'.pdf';\r\n  var smsshow = \"\";\r\n  if(sms==\"ok\"){\r\n    smsshow = \"ok\"\r\n  }else{\r\n    smsshow = \"\"};\r\n  if(success==\"true\"){\r\n     return(\r\n\r\n\r\n        <div class=\"row justify-content-md-center mt-5 pt-5\">\r\n  \r\n          <div class=\"col-lg-4 p-3 mt-5 \">\r\n          <div class=\"container\">\r\n              <div class=\"row\">\r\n                <div class=\"col-lg\">\r\n                  <div class=\"alert alert-success\" role=\"alert\">\r\n                  <i class=\"fas fa-check-circle\"></i>&nbsp;&nbsp;Payment is Succesful!\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              {smsshow?(\r\n\r\n                      <div class=\"row\">\r\n                      <div class=\"col-lg\">\r\n                        <div class=\"alert alert-success\" role=\"alert\">\r\n                        <i class=\"fas fa-envelope\"></i>&nbsp;&nbsp;Ticket details sent your mobile number (SMS)\r\n                        </div>\r\n                      </div>\r\n                      </div>\r\n              ):(\r\n\r\n                      <div class=\"row\">\r\n                      <div class=\"col-lg\">\r\n                        <div class=\"alert alert-warning\" role=\"alert\">\r\n                        <i class=\"fas fa-envelope\"></i>&nbsp;&nbsp;Ticket details can't sent your mobile number<br/>\r\n                        <div class=\"text-center\">\r\n                        <button class=\"btn btn-sm mt-2 btn-warning text-center\" onClick={this.sendsms}>Try Again</button>\r\n                        </div></div>\r\n                      </div>\r\n                      </div>\r\n\r\n              )}\r\n\r\n              